User Function MT110TOK()
Local lRet := .t.


If Altera .or. Inclui
	
	If EmptY(cUnidReq)
		Aviso("ATENCAO", "E obrigatorio a indentificacao da Unidade Requisitante.",{"&Ok"})
		lRet := .f.
	Else
		If EmptY(cCodCompr)
			Aviso("ATENCAO", "E obrigatorio a Indentificacao do Comprador.",{"&Ok"})
			lRet := .f.
		Endif
	Endif
	
Endif

Return(lRet)
