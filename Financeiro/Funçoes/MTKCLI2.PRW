User Function MTKCLI2() 

Local _aAreaP	:= GetArea()
Local _cSoma	:=	""

dbSelectArea("SA1")
_aAreaA1	:= GetArea()
dbSetOrder(1)       

If !dbSeek(xFilial("SA1"))
	_cSoma	:= "000001"
Else
	//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿
	//³SoftSeek, posiciona no Ulitmo Registro da Filial³
	//ÀÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÙ
	Set SoftSeek On
	dbSeek(xFilial("SA1")+"ZZZZZZ",.T.) 
	dbSkip(-1)
	Set SoftSeek Off

	_cSoma := Soma1(SA1->A1_COD)
Endif

RestArea(_aAreaA1)
RestArea(_aAreaP)

Return(_cSoma)